{
  "name": "ng2-xeditable",
  "version": "0.0.0",
  "description": "QuickStart package.json from the documentation, supplemented with testing support",
  "scripts": {
    "build": "tsc -p src/",
    "build:watch": "tsc -p src/ -w",
    "build:e2e": "tsc -p e2e/",
    "serve": "lite-server -c=bs-config.json",
    "serve:e2e": "lite-server -c=bs-config.e2e.json",
    "prestart": "npm run build",
    "start": "concurrently \"npm run build:watch\" \"npm run serve\" \"node ./server/www",
    "pree2e": "npm run build:e2e",
    "e2e": "concurrently \"npm run serve:e2e\" \"npm run protractor\" --kill-others --success first",
    "preprotractor": "webdriver-manager update",
    "protractor": "protractor protractor.config.js",
    "pretest": "npm run build",
    "test": "concurrently \"npm run build:watch\" \"karma start karma.conf.js\"",
    "pretest:once": "npm run build",
    "test:once": "karma start karma.conf.js --single-run",
    "lint": "tslint ./src/**/*.ts -t verbose"
  },
  "keywords": [],
  "author": "Dmitriy Shekhovtsov",
  "license": "MIT",
  "dependencies": {
    "@angular/common": "~2.4.0",
    "@angular/compiler": "~2.4.0",
    "@angular/core": "~2.4.0",
    "@angular/forms": "~2.4.0",
    "@angular/http": "~2.4.0",
    "@angular/material": "^2.0.0-beta.2",
    "@angular/platform-browser": "~2.4.0",
    "@angular/platform-browser-dynamic": "~2.4.0",
    "@angular/router": "~3.4.0",
    "angular-calendar": "^0.7.2",
    "angular-in-memory-web-api": "~0.2.4",
    "angular2-fullcalendar": "^1.1.1",
    "angular2-google-maps": "^0.17.0",
    "body-parser": "^1.17.1",
    "bootstrap": "3.3.7",
    "cordova": "^6.5.0",
    "core-js": "^2.4.1",
    "cors": "^2.8.1",
    "express": "^4.14.1",
    "fullcalendar": "^3.2.0",
    "hammerjs": "^2.0.8",
    "jquery": "^3.1.1",
    "multiple-date-picker": "^2.1.1",
    "multiple-date-picker-a2": "^2.5.3",
    "ng2-cc-library": "^1.0.1",
    "ng2-inline-editor": "^0.1.1-alpha.12",
    "ng2-table": "^1.3.2",
    "payform": "^1.2.1",
    "payu": "0.0.1",
    "primeng": "^2.0.3",
    "querystring": "^0.2.0",
    "rxjs": "^5.0.1",
    "systemjs": "0.19.40",
    "todomvc": "^0.1.1",
    "zone.js": "^0.7.4"
  },
  "devDependencies": {
    "concurrently": "^3.2.0",
    "lite-server": "^2.2.2",
    "typescript": "~2.0.10",
    "canonical-path": "0.0.2",
    "tslint": "^3.15.1",
    "lodash": "^4.16.4",
    "jasmine-core": "~2.4.1",
    "karma": "^1.3.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-jasmine": "^1.0.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~4.0.14",
    "rimraf": "^2.5.4",
    "@types/node": "^6.0.46",
    "ng2-bootstrap": "^1.3.3",
    "@types/jasmine": "2.5.36"
  },
  "repository": {},
  "main": "index.js"
}
